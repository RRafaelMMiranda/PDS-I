#include <stdio.h>

int main (void)
{
	int x0, x1, xn = 0;
	int n = 0;
	
	scanf("%i %i %i", &n, &x0, &x1);
	
	for (int i = 0; i <= n; i++)
	{
		if (i == 0)
		{
			printf("X0: %i\n", x0);
		}
		else if (i == 1)
		{
			printf("X1: %i\n", x1);
		}
		else
		{
			xn = (4 * x1) - (2 * x0);
			x0 = x1;
			x1 = xn;
			printf("X%i: %i\n", i, xn);
		}
	}
}
